{
  "multipart": [
    {"apply": {"model": "reframed:block/pane_post"}},
    {"when": {"north": "true"}, "apply": {"model": "reframed:block/pane_side"}},
    {"when": {"east": "true"}, "apply": {"model": "reframed:block/pane_side", "y": 90}},
    {"when": {"south": "true"}, "apply": {"model": "reframed:block/pane_side_alt"}},
    {"when": {"west": "true"}, "apply": {"model": "reframed:block/pane_side_alt", "y": 90}},
    {"when": {"north": "false"}, "apply": {"model": "reframed:block/pane_noside"}},
    {"when": {"east": "false"}, "apply": {"model": "reframed:block/pane_noside_alt"}},
    {"when": {"south": "false"}, "apply": {"model": "reframed:block/pane_noside_alt", "y": 90}},
    {"when": {"west": "false"}, "apply": {"model": "reframed:block/pane_noside", "y": 270}}
  ]
}
