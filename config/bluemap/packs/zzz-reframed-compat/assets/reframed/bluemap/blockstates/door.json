{
  "multipart": [
    {
      "apply": {
        "model": "reframed:block/trapdoor_open",
        "uvlock": true
      },
      "when": {
        "OR": [
          {
            "AND": [
              {
                "open": "false"
              },
              {
                "facing": "south"
              }
            ]
          },
          {
            "AND": [
              {
                "open": "true"
              },
              {
                "facing": "east"
              },
              {
                "hinge": "left"
              }
            ]
          },
          {
            "AND": [
              {
                "open": "true"
              },
              {
                "facing": "west"
              },
              {
                "hinge": "right"
              }
            ]
          }
        ]
      }
    },
