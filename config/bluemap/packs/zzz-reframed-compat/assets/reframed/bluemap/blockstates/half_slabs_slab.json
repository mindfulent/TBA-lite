{
  "multipart": [
    {
      "apply": {
        "model": "reframed:block/half_slabs_slab",
        "uvlock": true
      },
      "when": {
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "reframed:block/half_slabs_slab",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "reframed:block/half_slabs_slab",
        "uvlock": true,
        "x": 180
      },
      "when": {
        "facing": "up"
      }
    },
    {
      "apply": {
        "model": "reframed:block/half_slabs_slab",
        "uvlock": true,
        "x": 270
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "reframed:block/half_slabs_slab",
        "uvlock": true,
        "x": 90,
        "y": 90
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "reframed:block/half_slabs_slab",
        "uvlock": true,
        "x": 90,
        "y": 270
      },
      "when": {
        "facing": "east"
      }
    }
  ]
}