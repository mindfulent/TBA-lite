{
  "multipart": [
    {
      "apply": {
        "model": "reframed:block/pane_post",
        "uvlock": true
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_side_alt",
        "uvlock": true
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_side_alt",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_noside",
        "uvlock": true
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_noside_alt",
        "uvlock": true
      },
      "when": {
        "east": "false"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_noside_alt",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pane_noside",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "false"
      }
    }
  ]
}