{
  "multipart": [
    {
      "apply": {
        "model": "reframed:block/pillar",
        "uvlock": true,
        "x": 90,
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pillar",
        "uvlock": true
      },
      "when": {
        "axis": "y"
      }
    },
    {
      "apply": {
        "model": "reframed:block/pillar",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "axis": "z"
      }
    }
  ]
}