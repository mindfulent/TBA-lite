{
  "multipart": [
    {
      "apply": {
        "model": "reframed:block/fence_core",
        "uvlock": true
      }
    },
    {
      "apply": {
        "model": "reframed:block/post_fence_side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/post_fence_side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/post_fence_side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/post_fence_side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "reframed:block/fence_side_off",
        "uvlock": true
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "reframed:block/fence_side_off",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "false"
      }
    },
    {
      "apply": {
        "model": "reframed:block/fence_side_off",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "reframed:block/fence_side_off",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "false"
      }
    }
  ]
}